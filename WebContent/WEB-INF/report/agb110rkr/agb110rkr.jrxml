	<queryString language="SQL">
		<![CDATA[--agb110skr.Cagb110rkr[fnagb110r] Query2                          
SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
SELECT '' AS AC_DATE                                         
     , NULL AS SLIP_NUM                                      
     , NULL AS SLIP_SEQ                                      
     , CONVERT(NVARCHAR(20), N'이월금액') AS REMARK                                  
     , ACCNT                                                 
     , ISNULL((SELECT ACCNT_NAME                             
                 FROM ABA400T                                
                WHERE COMP_CODE = N'MASTER'
                  AND ACCNT     = A.ACCNT),'') AS ACCNT_NAME     
     , '' AS CUSTOM_NAME                                     
     , '' AS P_ACCNT_NAME                                    
     , ISNULL(SUM(DR_AMT_I),0.0) AS DR_AMT_I                 
     , ISNULL(SUM(CR_AMT_I),0.0) AS CR_AMT_I                 
     , ISNULL(CASE (SELECT JAN_DIVI                          
                      FROM ABA400T                           
                     WHERE COMP_CODE = N'MASTER'
                       AND ACCNT     = A.ACCNT)                  
                      WHEN '1'                               
                           THEN SUM(DR_AMT_I) - SUM(CR_AMT_I)
                      WHEN '2'                               
                           THEN SUM(CR_AMT_I) - SUM(DR_AMT_I)
               END,0.0) AS JAN_AMT_I                         
     , '' AS MONEY_UNIT                                      
     , 0 AS FOR_AMT_I                                        
     , '2' AS GUBUN                                          
     , 'N' AS POSTIT_YN                                      
     , '' AS POSTIT                                          
     , '' AS MOD_DIVI                                        
  FROM  (SELECT A.AC_DATE
              , B.ACCNT_CD ACCNT
              , A.DEPT_CODE
              , A.DR_AMT_I
              , A.CR_AMT_I
              , A.DIV_CODE
              , A.JAN_DIVI
              , INPUT_PATH
           FROM AGB100TV A
              , ABA400T  B 
          WHERE A.COMP_CODE = N'MASTER'
            AND B.COMP_CODE = N'MASTER'
            AND A.ACCNT     = B.ACCNT) A
     , BOR120T B                        
 WHERE B.COMP_CODE = N'MASTER'
   AND AC_DATE    >= N'201504'          
   AND A.DIV_CODE  = B.DIV_CODE                               
   AND AC_DATE     < N'20161101'        
   AND ((1=1
 )
 )
   AND A.DIV_CODE IN ('01')
 GROUP BY ACCNT, JAN_DIVI
 
 UNION ALL                                                   
 
SELECT '' AS AC_DATE                                         
     , NULL AS SLIP_NUM                                      
     , NULL AS SLIP_SEQ                                      
     , CONVERT(NVARCHAR(20), N'거래합계') AS REMARK                                  
     , ACCNT                                                 
     , ISNULL((SELECT ACCNT_NAME                             
                 FROM ABA400T                                
                WHERE COMP_CODE = N'MASTER'
                  AND ACCNT     = A.ACCNT),'') AS ACCNT_NAME     
     , '' AS CUSTOM_NAME                                     
     , '' AS P_ACCNT_NAME                                    
     , ISNULL(SUM(DR_AMT_I),0.0) AS DR_AMT_I                 
     , ISNULL(SUM(CR_AMT_I),0.0) AS CR_AMT_I                 
     , ISNULL(CASE (SELECT JAN_DIVI                          
                      FROM ABA400T                           
                     WHERE COMP_CODE = N'MASTER'
                       AND ACCNT     = A.ACCNT)                  
                      WHEN '1'                               
                           THEN SUM(DR_AMT_I) - SUM(CR_AMT_I)
                      WHEN '2'                               
                           THEN SUM(CR_AMT_I) - SUM(DR_AMT_I)
               END,0.0) AS JAN_AMT_I                         
     , '' AS MONEY_UNIT                                      
     , 0 AS FOR_AMT_I                                        
     , '2' AS GUBUN                                          
     , 'N' AS POSTIT_YN                                      
     , '' AS POSTIT                                          
     , '' AS MOD_DIVI                                        
  FROM  (SELECT A.AC_DATE
              , B.ACCNT_CD ACCNT
              , A.DEPT_CODE
              , A.DR_AMT_I
              , A.CR_AMT_I
              , A.DIV_CODE
              , A.JAN_DIVI
              , INPUT_PATH
           FROM AGB100TV A
              , ABA400T  B 
          WHERE A.COMP_CODE = N'MASTER'
            AND B.COMP_CODE = N'MASTER'
            AND A.ACCNT     = B.ACCNT) A
     , BOR120T B                        
 WHERE B.COMP_CODE  = N'MASTER'
   AND AC_DATE BETWEEN N'20161101'  
                   AND N'20161118'  
   AND INPUT_PATH   ='A1'                                       
   AND LEN(AC_DATE) = 6                                        
   AND A.DIV_CODE   = B.DIV_CODE                               
   AND ((1=1
 )
 )
   AND A.DIV_CODE IN ('01')
 GROUP BY ACCNT, JAN_DIVI
 HAVING SUM(DR_AMT_I) <> 0 OR SUM(CR_AMT_I) <>  0
 
 UNION ALL 
 
SELECT unilite.fnGetUserDateComp(N'MASTER', ac_date) AS AC_DATE                                                    
     , SLIP_NUM                                                                                     
     , SLIP_SEQ                                                                                     
     , ISNULL(SUBSTRING(REMARK, 1, 50),'') AS REMARK                                                
     , ACCNT                                                                                        
     , ISNULL((SELECT ACCNT_NAME                                                                    
                 FROM ABA400T                                                                       
                WHERE COMP_CODE = N'MASTER'
                  AND ACCNT     = A.ACCNT),'') AS ACCNT_NAME                                            
     , CASE WHEN ISNULL(A.CUSTOM_CODE,'') <> ''                                                        
                 THEN (SELECT CUSTOM_NAME                                                           
                         FROM BCM100T                                                               
                        WHERE COMP_CODE   = N'MASTER'
                          AND CUSTOM_CODE = A.CUSTOM_CODE)                                          
                 ELSE ISNULL(A.CUSTOM_NAME, '')                                                     
        END AS CUSTOM_NAME                                                                          
     , ISNULL((SELECT ACCNT_NAME                                                                    
                 FROM ABA400T                                                                       
                WHERE COMP_CODE = N'MASTER'
                  AND ACCNT     = A.P_ACCNT),'') AS P_ACCNT_NAME                                        
     , CASE DR_CR WHEN '1'                                                                          
                       THEN AMT_I                                                                   
                  WHEN '2'                                                                          
                       THEN 0.0                                                                     
        END AS DR_AMT_I                                                                             
     , CASE DR_CR WHEN '1'                                                                          
                       THEN 0.0                                                                     
                  WHEN '2'                                                                          
                       THEN AMT_I                                                                   
        END AS CR_AMT_I                                                                             
     , CASE JAN_DIVI WHEN DR_CR                                                                     
                          THEN AMT_I                                                                
                          ELSE -1 * AMT_I                                                           
        END AS JAN_AMT_I                                                                            
     , ISNULL(A.MONEY_UNIT, '') AS MONEY_UNIT                                                       
     , ISNULL(A.FOR_AMT_I, 0) AS FOR_AMT_I                                                          
     , '1' AS GUBUN                                                                                 
     , ISNULL(POSTIT_YN,'N') AS POSTIT_YN                                                           
     , ISNULL(POSTIT,'') AS POSTIT                                                                  
     , ISNULL(MOD_DIVI, 'N') AS MOD_DIVI                                                            
  FROM (SELECT DIV_CODE 
              , AC_DATE 
              , SLIP_NUM 
              , SLIP_SEQ
              , REMARK 
              , (SELECT ACCNT_CD
                   FROM ABA400T
                  WHERE COMP_CODE = N'MASTER'
                    AND ACCNT     = A.ACCNT) ACCNT
              , CUSTOM_CODE
              , CUSTOM_NAME
              , P_ACCNT
              , DEPT_CODE
              , DR_CR
              , AMT_I
              , JAN_DIVI
              , POSTIT_YN
              , POSTIT
              , MOD_DIVI
              , INPUT_PATH
              , CHARGE_CODE
              , MONEY_UNIT
              , FOR_AMT_I
           FROM AGJ210T A
          WHERE A.COMP_CODE = N'MASTER') A
     , BOR120T B                                                            
 WHERE B.COMP_CODE  = N'MASTER'
   AND AC_DATE BETWEEN N'20161101' AND N'20161118'
   AND A.DIV_CODE   = B.DIV_CODE                                                                      
   AND ((1=1
 )
 )

   AND A.DIV_CODE IN ('01')
   AND ISNULL(MOD_DIVI,'') = ''
 
 UNION ALL                                                                       
 
SELECT A.AC_DATE                                                                 
     , A.SLIP_NUM                                                                
     , A.SLIP_SEQ                                                                
     , A.REMARK                                                                  
     , A.ACCNT                                                                   
     , A.ACCNT_NAME                                                              
     , A.CUSTOM_NAME                                                             
     , A.P_ACCNT_NAME                                                            
     , A.DR_AMT_I                                                                
     , A.CR_AMT_I                                                                
     , A.JAN_AMT_I                                                               
     , A.MONEY_UNIT                                                              
     , A.FOR_AMT_I                                                               
     , A.GUBUN                                                                   
     , A.POSTIT_YN                                                               
     , A.POSTIT                                                                  
     , A.MOD_DIVI                                                                
  FROM (SELECT unilite.fnGetUserDateComp(N'MASTER', ac_date) AS AC_DATE                         
             , SLIP_NUM                                                          
             , SLIP_SEQ                                                          
             , ISNULL(SUBSTRING(REMARK, 1, 50),'') AS REMARK                                                
             , (SELECT ACCNT_CD                                                  
                  FROM ABA400T                                                   
                 WHERE COMP_CODE = N'MASTER'
                   AND ACCNT     =A.ACCNT) AS ACCNT                                   
             , ISNULL((SELECT ACCNT_NAME                                         
                         FROM ABA400T                                            
                        WHERE COMP_CODE = N'MASTER'
                          AND ACCNT     = (SELECT ACCNT_CD                           
                                             FROM ABA400T                            
                                            WHERE COMP_CODE = N'MASTER'
                                              AND ACCNT     = A.ACCNT)),'') AS ACCNT_NAME
             , CASE WHEN ISNULL(A.CUSTOM_NAME,'') = ''                           
                         THEN (SELECT CUSTOM_NAME                                
                                 FROM BCM100T                                    
                                WHERE COMP_CODE   = N'MASTER'
                                  AND CUSTOM_CODE = A.CUSTOM_CODE)               
                         ELSE ISNULL(A.CUSTOM_NAME, '')                          
                END AS CUSTOM_NAME                                               
             , ISNULL((SELECT ACCNT_NAME                                         
                         FROM ABA400T                                            
                        WHERE COMP_CODE = N'MASTER'
                          AND ACCNT     = A.P_ACCNT),'') AS P_ACCNT_NAME             
             , CASE DR_CR WHEN '1'                                               
                          THEN AMT_I                                        
                          WHEN '2'                                               
                          THEN 0.0                                          
                END AS DR_AMT_I                                                  
             , CASE DR_CR WHEN '1'                                               
                          THEN 0.0                                          
                          WHEN '2'                                               
                          THEN AMT_I                                        
                END AS CR_AMT_I                                                  
             , CASE JAN_DIVI WHEN DR_CR                                          
                             THEN AMT_I                                     
                             ELSE -1 * AMT_I                                
                END JAN_AMT_I                                                    
             , ISNULL(A.MONEY_UNIT, '') AS MONEY_UNIT                            
             , ISNULL(A.FOR_AMT_I, 0) AS FOR_AMT_I                               
             , '1' AS GUBUN                                                      
             , ISNULL(POSTIT_YN,'N') AS POSTIT_YN                                
             , ISNULL(POSTIT,'') AS POSTIT                                       
             , ISNULL(MOD_DIVI, 'N') AS MOD_DIVI                                 
          FROM (SELECT DIV_CODE 
              , AC_DATE 
              , SLIP_NUM 
              , SLIP_SEQ
              , REMARK 
              , (SELECT ACCNT_CD
                   FROM ABA400T
                  WHERE COMP_CODE = N'MASTER'
                    AND ACCNT     = A.ACCNT) ACCNT
              , CUSTOM_CODE
              , CUSTOM_NAME
              , P_ACCNT
              , DEPT_CODE
              , DR_CR
              , AMT_I
              , JAN_DIVI
              , POSTIT_YN
              , POSTIT
              , MOD_DIVI
              , INPUT_PATH
              , CHARGE_CODE
              , MONEY_UNIT
              , FOR_AMT_I
           FROM AGJ210T A
          WHERE A.COMP_CODE = N'MASTER') A                                
             , BOR120T B                                 
         WHERE B.COMP_CODE = N'MASTER'
           AND AC_DATE BETWEEN N'20161101' AND N'20161118'
           AND A.DIV_CODE  = B.DIV_CODE                                           
           AND (SELECT SUBJECT_DIVI                                              
                  FROM ABA400T                                                   
                 WHERE COMP_CODE = N'MASTER'
                   AND ACCNT     = A.ACCNT) = '2'                                    

   AND A.DIV_CODE IN ('01')
   AND ISNULL(MOD_DIVI,'') = ''
 ) A
 WHERE 1=1
   AND ((1=1
 )
 )
 ORDER BY ACCNT, AC_DATE, SLIP_NUM, SLIP_SEQ]]>
	</queryString>
