	<queryString>
		<![CDATA[SELECT A.COMP_CODE
			 , A.DIV_CODE
			 , A.TEAM_CODE
			 , A.TEAM_NAME
			 , A.STORE_CODE												AS  DEPT_CODE
			 , A.STORE_NAME												AS  DEPT_NAME
			 , SUM(A.LAST_YEAR_SALE_AMT_O) - SUM(A.LAST_YEAR_CM_FEE)	AS  LAST_YEAR_SALE_AMT_O		--전년도 매출액
			 , SUM(A.LAST_YEAR_CM_FEE)									AS	LAST_YEAR_CM_FEE			--전년도 위탁수수료
			 , SUM(A.LAST_YEAR_SALE_AMT_O)								AS	LAST_YEAR_SALE_AMT_SUM	    --전년도 총매출액
			 , SUM(A.SALE_AMT_O) - SUM(A.CM_FEE)						AS  SALE_AMT_O					--금년도 매출액
			 , SUM(A.CM_FEE)											AS	CM_FEE						--금년도 위탁수수료
			 , SUM(A.SALE_AMT_O)										AS	SALE_AMT_SUM				--금년도 총매출액
			 , SUM(A.SALE_AMT_O) - SUM(A.LAST_YEAR_SALE_AMT_O)			AS  INCREASE_DECREASE_O			--증감액
			 , CASE WHEN SUM(A.LAST_YEAR_SALE_AMT_O) = 0 THEN 0
					ELSE ((SUM(A.SALE_AMT_O) - SUM(A.LAST_YEAR_SALE_AMT_O)) / SUM(A.LAST_YEAR_SALE_AMT_O)) * 100
			   END														AS INCREASE_DECREASE_RATE		--증감율
			 , B.COMP_NAME 
			FROM(
				SELECT    A.COMP_CODE
						, A.DIV_CODE
						, E2.TREE_CODE AS TEAM_CODE
						, E2.TREE_NAME AS TEAM_NAME
						, E1.TREE_CODE AS STORE_CODE
						, E1.TREE_NAME AS STORE_NAME				
						, SUM(B.SALE_AMT_O + B.TAX_AMT_O + (ISNULL(B.DISCOUNT_P, 0) * B.SALE_Q)) AS LAST_YEAR_SALE_AMT_O  -- 총판매금액
						, SUM(CASE WHEN C.ITEM_ACCOUNT  = '02' OR C.ITEM_ACCOUNT  = '04' THEN (B.CONSIGNMENT_FEE * ABS(B.SALE_Q)) ELSE 0 END) AS LAST_YEAR_CM_FEE  -- 수탁수수료
						, 0 AS SALE_AMT_O
						, 0 AS CM_FEE
				  FROM			   SSA100T  A WITH(NOLOCK)
						INNER JOIN SSA110T  B WITH(NOLOCK) ON B.COMP_CODE = A.COMP_CODE
									 					  AND B.DIV_CODE  = A.DIV_CODE
														  AND B.BILL_NUM  = A.BILL_NUM
						INNER JOIN BPR200T  C WITH(NOLOCK) ON C.COMP_CODE = B.COMP_CODE
														  AND C.DIV_CODE  = B.DIV_CODE
														  AND C.ITEM_CODE = B.ITEM_CODE
						INNER JOIN BSA210T E1 WITH(NOLOCK) ON E1.COMP_CODE = A.COMP_CODE
														  AND E1.TYPE_LEVEL = A.DIV_CODE
														  AND E1.TREE_CODE = A.DEPT_CODE
						INNER JOIN BSA210T E2 WITH(NOLOCK) ON E2.COMP_CODE = E1.COMP_CODE
														  AND E2.TYPE_LEVEL = E1.TYPE_LEVEL
														  AND E2.TREE_LEVEL = SUBSTRING(E1.TREE_LEVEL, 1, 4)
		
				 WHERE A.COMP_CODE 	   = 'MASTER'
				   AND A.SALE_DATE >=  CONVERT(NVARCHAR(8),DATEADD(DAY, -10, '20151020'),112)
				   AND A.SALE_DATE <=  CONVERT(NVARCHAR(8),DATEADD(DAY, -10, '20151026'),112)
				    
				 GROUP BY A.COMP_CODE, A.DIV_CODE, E1.TREE_CODE, E1.TREE_NAME, E2.TREE_CODE, E2.TREE_NAME
		
				 UNION ALL
		
				 SELECT    A.COMP_CODE
						, A.DIV_CODE
						, E2.TREE_CODE AS TEAM_CODE
						, E2.TREE_NAME AS TEAM_NAME
						, E1.TREE_CODE AS STORE_CODE
						, E1.TREE_NAME AS STORE_NAME	
						, 0 AS LAST_YEAR_SALE_AMT_O
						, 0 AS LAST_YEAR_CM_FEE			
						, SUM(B.SALE_AMT_O + B.TAX_AMT_O + (ISNULL(B.DISCOUNT_P, 0) * B.SALE_Q)) AS SALE_AMT_O  -- 총판매금액
						, SUM(CASE WHEN C.ITEM_ACCOUNT  = '02' OR C.ITEM_ACCOUNT  = '04' THEN (B.CONSIGNMENT_FEE * ABS(B.SALE_Q)) ELSE 0 END) AS CM_FEE  -- 수탁수수료
				  FROM			   SSA100T  A WITH(NOLOCK)
						INNER JOIN SSA110T  B WITH(NOLOCK) ON B.COMP_CODE = A.COMP_CODE
									 					  AND B.DIV_CODE  = A.DIV_CODE
														  AND B.BILL_NUM  = A.BILL_NUM
						INNER JOIN BPR200T  C WITH(NOLOCK) ON C.COMP_CODE = B.COMP_CODE
														  AND C.DIV_CODE  = B.DIV_CODE
														  AND C.ITEM_CODE = B.ITEM_CODE
						INNER JOIN BSA210T E1 WITH(NOLOCK) ON E1.COMP_CODE = A.COMP_CODE
														  AND E1.TYPE_LEVEL = A.DIV_CODE
														  AND E1.TREE_CODE = A.DEPT_CODE
						INNER JOIN BSA210T E2 WITH(NOLOCK) ON E2.COMP_CODE = E1.COMP_CODE
														  AND E2.TYPE_LEVEL = E1.TYPE_LEVEL
														  AND E2.TREE_LEVEL = SUBSTRING(E1.TREE_LEVEL, 1, 4)
		
				 WHERE A.COMP_CODE 	   = 'MASTER'
				   AND A.SALE_DATE >= '20151020'
				   AND A.SALE_DATE <= '20151026'
				    
				 GROUP BY A.COMP_CODE, A.DIV_CODE, E1.TREE_CODE, E1.TREE_NAME, E2.TREE_CODE, E2.TREE_NAME
		
			 )A		


			 LEFT  JOIN BOR100T 		  B WITH (NOLOCK) ON A.COMP_CODE  = B.COMP_CODE  
		 GROUP BY A.COMP_CODE, A.DIV_CODE, A.TEAM_CODE, A.TEAM_NAME, A.STORE_CODE, A.STORE_NAME ,B.COMP_NAME]]>
	</queryString>
