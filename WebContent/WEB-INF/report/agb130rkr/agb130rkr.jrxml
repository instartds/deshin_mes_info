	   AND A.DIV_CODE IN
		 (  
			NULL
		 )          
	 
	
	    AND   ((A.BOOK_DATA1             = ISNULL(@BOOK_DATA1, '')               AND ISNULL(@BOOK_DATA1, '')   != '' AND ISNULL(@BOOK_NAME1, '') != '') OR
