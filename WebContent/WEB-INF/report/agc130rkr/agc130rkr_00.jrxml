                SELECT @SUM_AMT_I = SUM(C.AMT_I_BEGIN)
                FROM ABA120T A
                LEFT JOIN ABA130T B WITH (NOLOCK) ON A.COMP_CODE = B.COMP_CODE
                                                 AND A.GUBUN     = B.GUBUN
                                                 AND A.DIVI      = B.DIVI
                                                 AND A.ACCNT_CD  = B.ACCNT_CD
                LEFT JOIN TMP_ABA107T C WITH (NOLOCK) ON A.COMP_CODE = C.COMP_CODE
                                                     AND B.ACCNT     = C.ACCNT_CD
                WHERE A.COMP_CODE   = @COMP_CODE
                  AND A.DIVI        = N'10'
                  AND A.OPT_DIVI    = N'2'
                  AND A.ACCNT_CD    = @ACCNT_CD
                  GROUP BY A.COMP_CODE, A.GUBUN, A.DIVI, A.ACCNT_CD
                  
                IF @SUM_AMT_I != 0
                    BEGIN
                        UPDATE TMP_ABA107T SET AMT_I_BEGIN = @SUM_AMT_I
                         WHERE ACCNT_CD = @ACCNT_CD
                    END                 
                
                --------------------------------------
                ----合计数  年末    part two
                --------------------------------------
                SET @SUM_AMT_I = 0                  
                SELECT @SUM_AMT_I = SUM(C.AMT_I_END)
                FROM ABA120T A
                LEFT JOIN ABA130T B WITH (NOLOCK) ON A.COMP_CODE = B.COMP_CODE
                                                 AND A.GUBUN     = B.GUBUN
                                                 AND A.DIVI      = B.DIVI
                                                 AND A.ACCNT_CD  = B.ACCNT_CD
                LEFT JOIN TMP_ABA107T C WITH (NOLOCK) ON A.COMP_CODE = C.COMP_CODE
                                                     AND B.ACCNT     = C.ACCNT_CD
                WHERE A.COMP_CODE   = @COMP_CODE
                  AND A.DIVI        = N'10'
                  AND A.OPT_DIVI    = N'2'
                  AND A.ACCNT_CD    = @ACCNT_CD
                  GROUP BY A.COMP_CODE, A.GUBUN, A.DIVI, A.ACCNT_CD
                  
                IF @SUM_AMT_I != 0
                    BEGIN
                        UPDATE TMP_ABA107T SET AMT_I_END = @SUM_AMT_I
                         WHERE ACCNT_CD = @ACCNT_CD
                    END             
                                
                --------------------------------------
                ----合计数  年初      part two
                --------------------------------------
                SET @SUM_AMT_I = 0
                SELECT @SUM_AMT_I = SUM(C.AMT_I_BEGIN)
                FROM ABA120T A
                LEFT JOIN ABA130T B WITH (NOLOCK) ON A.COMP_CODE = B.COMP_CODE
                                                 AND A.GUBUN     = B.GUBUN
                                                 AND A.DIVI      = B.DIVI
                                                 AND A.ACCNT_CD  = B.ACCNT_CD
                LEFT JOIN TMP_ABA107T C WITH (NOLOCK) ON A.COMP_CODE = C.COMP_CODE
                                                     AND B.ACCNT     = C.ACCNT_CD
                WHERE A.COMP_CODE   = @COMP_CODE
                  AND A.DIVI        = N'10'
                  AND A.OPT_DIVI    = N'2'
                  AND A.ACCNT_CD    = @ACCNT_CD
                  GROUP BY A.COMP_CODE, A.GUBUN, A.DIVI, A.ACCNT_CD
                  
                IF @SUM_AMT_I != 0
                    BEGIN
                        UPDATE TMP_ABA107T SET AMT_I_BEGIN = @SUM_AMT_I
                         WHERE ACCNT_CD = @ACCNT_CD
                    END                 
                
                SET @ICOUNT = @ICOUNT + 1
            END
    END

        SELECT @COUNTM1 = COUNT(*)
        FROM TMP_ABA107T
        WHERE KIND_DIVI = N'60'

        SELECT @COUNTM2 = COUNT(*)
        FROM TMP_ABA107T
        WHERE KIND_DIVI IN (N'70',N'80')
        
        SET @COUNTM3 = @COUNTM1 - @COUNTM2
        
    IF @COUNTM3 > 0
        BEGIN
            SET @ICOUNT = 1
            WHILE @ICOUNT <= @COUNTM3
                BEGIN
                    INSERT INTO TMP_ABA107T(COMP_CODE, ACCNT_CD, GUBUN, DIVI, SEQ, ACCNT_NAME,
                        LINE_NUM, KIND_DIVI, AMT_I_BEGIN, AMT_I_END, REPORT_DATE, COMP_NAME) 
                    VALUES(@COMP_CODE, N'', N'', N'', 0, N'', N'', N'00', 0, 0, @TO_DATE, @COMP_NAME)
                    SET @ICOUNT = @ICOUNT + 1
                END
        END
        
        
        
        
        
/**************************2. 查询****************************/
--s_agc180skr_cn.Cagc180skr[fnagc180r1] Query BL
        SELECT A.COMP_CODE                  AS COMP_CODE1
             , A.ACCNT_CD                   AS ACCNT_CD1
             , A.GUBUN                      AS GUBUN1
             , A.DIVI                       AS DIVI1
             , A.SEQ                        AS SEQ1
             , A.ACCNT_NAME                 AS ACCNT_NAME1
             , ISNULL(A.LINE_NUM,'-')       AS LINE_NUM1
             , A.KIND_DIVI                  AS KIND_DIVI1
             , A.AMT_I_BEGIN                AS AMT_I_BEGIN1
             , A.AMT_I_END                  AS AMT_I_END1
             , A.REPORT_DATE                AS REPORT_DATE1
             , A.COMP_NAME                  AS COMP_NAME1
             , T1.COMP_CODE                 AS COMP_CODE2
             , T1.ACCNT_CD                  AS ACCNT_CD2
             , T1.GUBUN                     AS GUBUN2
             , T1.DIVI                      AS DIVI2
             , T1.SEQ                       AS SEQ2
             , T1.ACCNT_NAME                AS ACCNT_NAME2
             , (CASE ISNULL(T1.LINE_NUM,'') WHEN N'' THEN '-'
                                            ELSE T1.LINE_NUM END)   AS LINE_NUM2
             , T1.KIND_DIVI                 AS KIND_DIVI2
             , T1.AMT_I_BEGIN               AS AMT_I_BEGIN2
             , T1.AMT_I_END                 AS AMT_I_END2
             , T1.REPORT_DATE               AS REPORT_DATE2
             , T1.COMP_NAME                 AS COMP_NAME2
             , convert(nvarchar(4),YEAR(a.REPORT_DATE),112)+N' 年'+ convert(nvarchar(4),MONTH (a.REPORT_DATE),112) +N'月' AS REPORT_MONTH
             , N'元'                                   AS MONEY_UNIT
        FROM TMP_ABA107T A
        LEFT JOIN (SELECT ROW_NUMBER() OVER (ORDER BY T.COMP_CODE) AS ID
                    , *
                    FROM
                    (SELECT * FROM TMP_ABA107T WHERE KIND_DIVI = N'70'
                    UNION ALL
                    SELECT * FROM TMP_ABA107T WHERE KIND_DIVI = N'00'
                    UNION ALL
                    SELECT * FROM TMP_ABA107T WHERE KIND_DIVI = N'80') T) T1 ON A.COMP_CODE = T1.COMP_CODE
                                                                            AND T1.ID       = A.ISEQ
        WHERE A.KIND_DIVI = N'60']]>
	</queryString>
