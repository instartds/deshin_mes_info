--s_agc180skr_cn.Cagc180skr[fnagc180r1] Query BALANCE
SET NOCOUNT ON
DECLARE @COMP_CODE      NVARCHAR(16)
DECLARE @FR_DATE        NVARCHAR(8)---最早日期
DECLARE @TO_DATE        NVARCHAR(8)---上年度
DECLARE @FR_DATE_LAST   NVARCHAR(8)---最早日期
DECLARE @TO_DATE_LAST   NVARCHAR(8)---上年度
DECLARE @ICOUNT         INT
DECLARE @MAXCOUNT       INT
DECLARE @SUM_AMT_I      NUMERIC(30,6)
DECLARE @SUM_AMT_I_ADD  NUMERIC(30,6)
DECLARE @COMP_NAME      NVARCHAR(60)
DECLARE @ACCNT_CD       NVArchar(32)
DECLARE @SEQ            NUMERIC(5,0)
DECLARE @BEGIN_DATE     NVARCHAR(8)
DECLARE @COUNTM1        INT
DECLARE @COUNTM2        INT
DECLARE @COUNTM3        INT
 
DECLARE @DATE_MONTH     NVARCHAR(8)
SET     @DATE_MONTH     = N'201601'
SET     @COMP_CODE      = N'MASTER'

----------
--日期转换
----------
SELECT TOP 1 @FR_DATE_LAST = ESTABLISH_DATE FROM BOR100T
SELECT TOP 1 @FR_DATE      = ESTABLISH_DATE FROM BOR100T
SELECT TOP 1 @COMP_NAME    = COMP_NAME FROM BOR100T WHERE COMP_CODE = @COMP_CODE
SET @TO_DATE               = @DATE_MONTH + N'01'
SET @TO_DATE               = CONVERT(NVARCHAR(8),DATEADD(MONTH, 1, CONVERT(DATETIME, @TO_DATE)),112)
SET @TO_DATE               = CONVERT(NVARCHAR(8),DATEADD(DAY, -1, CONVERT(DATETIME, @TO_DATE)),112)
SET @TO_DATE_LAST          = LEFT(@DATE_MONTH,4) + N'01' + N'01'
SET @TO_DATE_LAST          = CONVERT(NVARCHAR(8),DATEADD(DAY, -1, CONVERT(DATETIME, @TO_DATE_LAST)),112)
SET @BEGIN_DATE            = LEFT(@FR_DATE,4) + N'01'


IF EXISTS(SELECT * FROM SYSOBJECTS WHERE ID = OBJECT_ID('TMP_ABA107T'))
    BEGIN
        DROP TABLE TMP_ABA107T
    END
        
CREATE TABLE TMP_ABA107T
    (
    ISEQ            INT IDENTITY(1,1)
  , COMP_CODE       NVARCHAR(16)  COLLATE Korean_Wansung_CI_AS
  , ACCNT_CD        NVARCHAR(32)  COLLATE Korean_Wansung_CI_AS
  , GUBUN           NVARCHAR(4)   COLLATE Korean_Wansung_CI_AS
  , DIVI            NVARCHAR(4)   COLLATE Korean_Wansung_CI_AS
  , SEQ             NUMERIC(5,0) 
  , ACCNT_NAME      NVARCHAR(200) COLLATE Korean_Wansung_CI_AS
  , LINE_NUM        NVARCHAR(5)   COLLATE Korean_Wansung_CI_AS
  , KIND_DIVI       NVARCHAR(4)   COLLATE Korean_Wansung_CI_AS
  , AMT_I_BEGIN     NUMERIC(30,6)
  , AMT_I_END       NUMERIC(30,6)
  , REPORT_DATE     NVARCHAR(8)   COLLATE Korean_Wansung_CI_AS
  , COMP_NAME       NVARCHAR(60)  COLLATE Korean_Wansung_CI_AS
    )
    
INSERT INTO TMP_ABA107T
    (COMP_CODE, ACCNT_CD, GUBUN, DIVI, SEQ, ACCNT_NAME, LINE_NUM, KIND_DIVI, AMT_I_BEGIN, AMT_I_END, REPORT_DATE, COMP_NAME)

SELECT A.COMP_CODE      AS COMP_CODE
     , A.ACCNT_CD       AS ACCNT_CD
     , A.GUBUN          AS GUBUN
     , A.DIVI           AS DIVI
     , A.SEQ            AS SEQ
     , A.ACCNT_NAME     AS ACCNT_NAME
     , A.LINE_NUM       AS LINE_NUM
     , A.KIND_DIVI      AS KIND_DIVI
     , 0                AS AMT_I_BEGIN
     , 0                AS AMT_I_END
     , @TO_DATE         AS REPORT_DATE
     , @COMP_NAME       AS COMP_NAME
FROM ABA120T A
WHERE DIVI = '10'
  AND COMP_CODE = @COMP_CODE
  
  EXEC uniLITE.SP_ACCNT_FC_DATA2 @COMP_CODE, @ACCNT_CD, '1', @FR_DATE, @TO_DATE, @TO_DATE_LAST
  EXEC uniLITE.SP_ACCNT_FC_DATA1 @COMP_CODE, @ACCNT_CD, '0', @FR_DATE, @TO_DATE, @TO_DATE_LAST
------------------------------------
--MAXCOUNT
------------------------------------
SELECT @MAXCOUNT = COUNT(*) FROM TMP_ABA107T
IF @MAXCOUNT > 0
    BEGIN
        SET @ICOUNT = 1
        WHILE @ICOUNT <= @MAXCOUNT
            BEGIN
                SELECT @ACCNT_CD  = ACCNT_CD
                     , @COMP_CODE = COMP_CODE
                     , @SEQ       = SEQ
                FROM TMP_ABA107T
                WHERE ISEQ = @ICOUNT
                -------------------------
                --一般项 AMT_I
                -------------------------                 
                SET @SUM_AMT_I = 0
                SELECT @SUM_AMT_I =
                SUM(CASE WHEN C.JAN_DIVI = '1' AND B.CAL_DIVI = '1' THEN ISNULL(T.AMT_I,0) + ISNULL(T1.SUM_ORI_AMT_I,0)
                         WHEN C.JAN_DIVI = '1' AND B.CAL_DIVI = '2' THEN ISNULL(T.AMT_I,0) * (-1) + ISNULL(T1.SUM_ORI_AMT_I,0) * (-1)
                         WHEN C.JAN_DIVI = '2' AND B.CAL_DIVI = '2' THEN ISNULL(T.AMT_I,0) + ISNULL(T1.SUM_ORI_AMT_I,0)
                         WHEN C.JAN_DIVI = '2' AND B.CAL_DIVI = '1' THEN ISNULL(T.AMT_I,0) * (-1) + ISNULL(T1.SUM_ORI_AMT_I,0) * (-1) END)
                FROM ABA120T A
                LEFT JOIN ABA130T B WITH (NOLOCK) ON A.COMP_CODE = B.COMP_CODE
                                                 AND A.GUBUN     = B.GUBUN
                                                 AND A.DIVI      = B.DIVI
                                                 AND A.ACCNT_CD  = B.ACCNT_CD
                LEFT JOIN ABA400T C WITH (NOLOCK) ON A.COMP_CODE = C.COMP_CODE
                                                 AND B.ACCNT     = C.ACCNT
                LEFT JOIN (SELECT A.COMP_CODE
                            , SUM(CASE A.DR_CR WHEN '1' THEN A.AMT_I
                                               WHEN '2' THEN A.AMT_I * (-1) END) AS AMT_I
                            , LEFT(A.ACCNT,4) AS MAIN_ACCNT
                            FROM AGJ210T A
                            LEFT JOIN ABA400T B WITH (NOLOCK) ON A.COMP_CODE = B.COMP_CODE
                                                             AND A.ACCNT     = B.ACCNT
                            WHERE A.AC_DATE            >= @FR_DATE
                              AND A.AC_DATE            <= @TO_DATE
                              AND A.COMP_CODE           = @COMP_CODE
                              AND ISNULL(A.MOD_DIVI,'') = ''
                            GROUP BY A.COMP_CODE, LEFT(A.ACCNT,4)) T ON A.COMP_CODE        = T.COMP_CODE
                                                                    AND ISNULL(B.ACCNT,'') = T.MAIN_ACCNT
                LEFT JOIN (SELECT C.COMP_CODE
                                , LEFT(C.ACCNT,4) AS ACCNT
                                , C.DIV_CODE
                                , C.DEPT_CODE
                                , SUM(CASE WHEN C.DR_AMT_I <> 0 THEN C.DR_AMT_I
                                           WHEN C.DR_AMT_I =  0 THEN C.CR_AMT_I * (-1) END) AS SUM_ORI_AMT_I
                             FROM AGJ800T C
                            WHERE C.COMP_CODE = @COMP_CODE
                             AND (C.DR_AMT_I <> 0 OR C.CR_AMT_I <>0)
                            GROUP BY C.COMP_CODE, LEFT(C.ACCNT,4), C.DIV_CODE, C.DEPT_CODE) T1 ON A.COMP_CODE = T1.COMP_CODE
                                                                                AND B.ACCNT     = T1.ACCNT
                WHERE A.DIVI     = N'10'
                  AND A.COMP_CODE = @COMP_CODE
                  AND A.OPT_DIVI IN (N'1',N'5')
                  AND A.ACCNT_CD = @ACCNT_CD
                GROUP BY A.COMP_CODE, A.GUBUN, A.DIVI, A.ACCNT_CD
                
                IF @SUM_AMT_I != 0
                    BEGIN
                        UPDATE TMP_ABA107T SET AMT_I_END = @SUM_AMT_I
                        WHERE ACCNT_CD = @ACCNT_CD
                    END
                --------------------------------------
                -----一般项年初余额
                --------------------------------------
                SET @SUM_AMT_I = 0
                --SELECT @SUM_AMT_I = SUM(ISNULL(T.AMT_I,0) + ISNULL(T1.SUM_ORI_AMT_I,0))
                SELECT @SUM_AMT_I = 
                SUM(CASE WHEN C.JAN_DIVI = '1' AND B.CAL_DIVI = '1' THEN ISNULL(T.AMT_I,0) + ISNULL(T1.SUM_ORI_AMT_I,0)
                         WHEN C.JAN_DIVI = '1' AND B.CAL_DIVI = '2' THEN ISNULL(T.AMT_I,0) * (-1) + ISNULL(T1.SUM_ORI_AMT_I,0) * (-1)
                         WHEN C.JAN_DIVI = '2' AND B.CAL_DIVI = '2' THEN ISNULL(T.AMT_I,0) + ISNULL(T1.SUM_ORI_AMT_I,0)
                         WHEN C.JAN_DIVI = '2' AND B.CAL_DIVI = '1' THEN ISNULL(T.AMT_I,0) * (-1) + ISNULL(T1.SUM_ORI_AMT_I,0) * (-1) END)
                FROM ABA120T A
                LEFT JOIN ABA130T B WITH (NOLOCK) ON A.COMP_CODE = B.COMP_CODE
                                                 AND A.GUBUN     = B.GUBUN
                                                 AND A.DIVI      = B.DIVI
                                                 AND A.ACCNT_CD  = B.ACCNT_CD
                LEFT JOIN ABA400T C WITH (NOLOCK) ON A.COMP_CODE = C.COMP_CODE
                                                 AND B.ACCNT     = C.ACCNT
                LEFT JOIN (SELECT A.COMP_CODE
                            , SUM(CASE A.DR_CR WHEN '1' THEN A.AMT_I
                                               WHEN '2' THEN A.AMT_I * (-1) END) AS AMT_I
                            , LEFT(A.ACCNT,4) AS MAIN_ACCNT
                            FROM AGJ210T A
                            LEFT JOIN ABA400T B WITH (NOLOCK) ON A.COMP_CODE = B.COMP_CODE
                                                             AND A.ACCNT     = B.ACCNT
                            WHERE A.AC_DATE            >= @FR_DATE_LAST 
                              AND A.AC_DATE            <= @TO_DATE_LAST
                              AND A.COMP_CODE           = @COMP_CODE
                              AND ISNULL(A.MOD_DIVI,'') = ''
                            GROUP BY A.COMP_CODE, LEFT(A.ACCNT,4)) T ON A.COMP_CODE        = T.COMP_CODE
                                                                    AND ISNULL(B.ACCNT,'') = T.MAIN_ACCNT
                LEFT JOIN (  SELECT C.COMP_CODE
                           , LEFT(C.ACCNT,4) AS ACCNT
                           , C.DIV_CODE
                           , C.DEPT_CODE
                           , SUM(CASE WHEN C.DR_AMT_I <> 0 THEN C.DR_AMT_I
                                      WHEN C.DR_AMT_I =  0 THEN C.CR_AMT_I * (-1) END) AS SUM_ORI_AMT_I
                             FROM AGJ800T C
                            WHERE  C.COMP_CODE = @COMP_CODE
                              AND (C.DR_AMT_I <> 0 OR C.CR_AMT_I <>0)
                            GROUP BY C.COMP_CODE, LEFT(C.ACCNT,4), C.DIV_CODE, C.DEPT_CODE) T1 ON A.COMP_CODE = T1.COMP_CODE
                                                                  AND B.ACCNT     = T1.ACCNT
                WHERE A.DIVI     = N'10'
                  AND A.COMP_CODE = @COMP_CODE
                  AND A.OPT_DIVI IN (N'1',N'5')
                  AND A.ACCNT_CD = @ACCNT_CD
                GROUP BY A.COMP_CODE, A.GUBUN, A.DIVI, A.ACCNT_CD
                
