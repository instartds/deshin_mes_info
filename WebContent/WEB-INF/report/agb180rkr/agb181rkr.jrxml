	<queryString language="SQL">
		<![CDATA[SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
		    SELECT '1' AS GRP                                                                               
		         , A.PEND_CD                                                                                
		         , A.PEND_NAME                                                                              
		         , A.ACCNT                                                                                  
		         , A.ACCNT_NAME                                                                             
		         , SUM(ISNULL(A.WAL_AMT_I, 0)) AS WAL_AMT_I                                                 
		         , SUM(ISNULL(A.BUSI_AMT_I,0)) AS BUSI_AMT_I                                                
		         , SUM(ISNULL(A.DR_AMT_I, 0))  AS DR_AMT_I                                                  
		         , SUM(ISNULL(A.CR_AMT_I, 0))  AS CR_AMT_I                                                  
		         , SUM(ISNULL(A.JAN_AMT_I, 0)) AS JAN_AMT_I                                                 
		      FROM (SELECT ISNULL(CASE WHEN (ISNULL(B.BOOK_CODE1, '') = 'A4'  
		     
		     
		                                  ) THEN ISNULL(B.BOOK_DATA1, '')                                   
		                               WHEN (ISNULL(B.BOOK_CODE2, '') = 'A4'     
		     
		     
		                                  ) THEN ISNULL(B.BOOK_DATA2, '')                                   
		                          END, '') AS PEND_CD                                                       
		    
		                 , ISNULL(CASE WHEN (ISNULL(B.BOOK_CODE1, '') = 'A4'   
		     
		     
		                                    ) THEN (SELECT AC_NAME                                          
		                                              FROM ABA200TV                                         
		                                             WHERE COMP_CODE = 'MASTER'
		                                               AND AC_CODE   = ISNULL(B.BOOK_CODE1, '') 
		                                               AND AC_DATA   = ISNULL(B.BOOK_DATA1, ''))
		                               WHEN (ISNULL(B.BOOK_CODE2, '') = 'A4'
		     
		     
		                                    ) THEN (SELECT AC_NAME                                          
		                                              FROM ABA200TV                                         
		                                             WHERE COMP_CODE = 'MASTER'
		                                               AND AC_CODE   = ISNULL(B.BOOK_CODE2, '')               
		                                               AND AC_DATA   = ISNULL(B.BOOK_DATA2, ''))              
		                            End,'') AS PEND_NAME                                                    
		    
		                 , A.ACCNT                                                                          
		                                                                                   
		                 , A.ACCNT_NAME   AS ACCNT_NAME 
		                  
		                  
		                                    
		                 , ISNULL((SELECT CASE B.JAN_DIVI WHEN '1' THEN SUM(ISNULL(B.DR_FOR_AMT_I, 0)) - SUM(ISNULL(B.CR_FOR_AMT_I, 0))
		                                                  WHEN '2' THEN SUM(ISNULL(B.CR_FOR_AMT_I, 0)) - SUM(ISNULL(B.DR_FOR_AMT_I, 0))
		                                  END                                                               
		                             FROM ABA400T 
		                            WHERE COMP_CODE = 'MASTER'
		                              AND ACCNT     = B.ACCNT                                     
		    -- sSqlA /* 출력 A 조건이 들어가야함 */
		    						  AND (( B.BOOK_CODE1 = 'A4'                                 
						     
						     
					              )                                                                                   
						           OR ( B.BOOK_CODE2 = 'A4'     
						                                   
						     
					              ))                                                                                  
						     					
				        			  AND     B.DIV_CODE IN
									 (  
									 '01'
									 ) 									
				  	 		 	  	 		
				  	 		 
						  	 
				                      AND B.AC_DATE >= '201504'
				                      AND B.AC_DATE <= '20160801'
									  AND B.INPUT_PATH != 'A1'
		    	---- 출력 A 조건 끝 -----
		    
		    
		                          ), 0) AS WAL_FOR_AMT_I                                                    
		                 , ISNULL((SELECT CASE B.JAN_DIVI WHEN '1' THEN SUM(ISNULL(B.DR_AMT_I, 0)) - SUM(ISNULL(B.CR_AMT_I, 0)) 
		                                                  WHEN '2' THEN SUM(ISNULL(B.CR_AMT_I, 0)) - SUM(ISNULL(B.DR_AMT_I, 0)) 
		                                  END                                                               
		                             FROM ABA400T                                                           
		                            WHERE COMP_CODE = 'MASTER'
		                              AND ACCNT     = B.ACCNT                                                   
		    -- sSqlA /* 출력 A 조건 */
		    						  AND (( B.BOOK_CODE1 = 'A4'                                 
						     
						     
					              )                                                                                   
						           OR ( B.BOOK_CODE2 = 'A4'     
						                                   
						     
					              ))                                                                                  
						     					
				        			  AND     B.DIV_CODE IN
									 (  
									 '01'
									 ) 									
				  	 		 	  	 		
				  	 		 
						  	 
				                      AND B.AC_DATE >= '201504'
				                      AND B.AC_DATE <= '20160801'
									  AND B.INPUT_PATH != 'A1'
		    	---- 출력 A 조건 끝 -----
		                          ), 0) AS WAL_AMT_I                                                        
		                 , ISNULL((SELECT SUM(ISNULL(B.DR_FOR_AMT_I, 0))                                    
		                             FROM ABA400T                                                           
		                            WHERE COMP_CODE = 'MASTER'
		                              AND ACCNT     = B.ACCNT                                                   
		        -- sSqlB /* 출력 B 조건이 들어가야함 */
							         AND (( B.BOOK_CODE1 =  'A4'
					           
					           
					              	)                                                                                   
				           		OR (B.BOOK_CODE2 = 'A4'    
					           
					           
					              ))
					           					
			        				 AND     B.DIV_CODE IN
										 (  
										  '01'
										 ) 									
					 		                                                                                         
					    	   
					    	   
					          		 AND B.AC_DATE >= '20160801'                                  
					          		 AND B.AC_DATE <= '20160816'                                      
					          		 AND B.INPUT_PATH != 'A1' 
         				 --- 출력 B 조건 끝 ----
		                          ), 0) AS DR_FOR_AMT_I                                                     
		                 , ISNULL((SELECT CASE B.JAN_DIVI WHEN '1' THEN SUM(ISNULL(B.DR_AMT_I, 0)) - SUM(ISNULL(B.CR_AMT_I, 0)) 
		                                                  WHEN '2' THEN SUM(ISNULL(B.CR_AMT_I, 0)) - SUM(ISNULL(B.DR_AMT_I, 0)) 
		                                  END                                                               
		                             FROM ABA400T                                                           
		                            WHERE COMP_CODE = 'MASTER'
		                              AND ACCNT     = B.ACCNT                                                   
		       -- sSqlC /* 출력 C 조건*/
		    						  AND (( B.BOOK_CODE1 = 'A4'                                 
						     
						     
						              )                                                                                   
						           OR ( B.BOOK_CODE2 = 'A4'     
						                                   
						     
						              ))                                                                                  
						     					
			        				  AND     B.DIV_CODE IN
								 (  
								 '01'
								 ) 									
				  	 		 	  	 		
				  	 		 
						  	 
									  AND B.INPUT_PATH = 'A1'                                                                   
							          AND B.AC_DATE BETWEEN '20160801' AND '20160816'
					          
					    --- 출력 C 조건 끝      
					          
		                          ), 0) AS BUSI_AMT_I                                                       
		                 , ISNULL((SELECT SUM(ISNULL(B.DR_AMT_I, 0))                                        
		                             FROM ABA400T                                                           
		                            WHERE COMP_CODE = 'MASTER'
		                              AND ACCNT     = B.ACCNT                                                   
		    -- sSqlB /* 출력 B 조건이 들어가야함 */
							         AND (( B.BOOK_CODE1 =  'A4'
					           
					           
					              	)                                                                                   
				           		OR (B.BOOK_CODE2 = 'A4'    
					           
					           
					              ))
					           					
			        				 AND     B.DIV_CODE IN
										 (  
										  '01'
										 ) 									
					 		                                                                                         
					    	   
					    	   
					          		 AND B.AC_DATE >= '20160801'                                  
					          		 AND B.AC_DATE <= '20160816'                                      
					          		 AND B.INPUT_PATH != 'A1' 
         				 --- 출력 B 조건 끝 ----
		                          ), 0) AS DR_AMT_I                                                         
		                 , ISNULL((SELECT SUM(ISNULL(B.CR_FOR_AMT_I, 0))                                    
		                             FROM ABA400T                                                           
		                            WHERE COMP_CODE = 'MASTER'
		                              AND ACCNT     = B.ACCNT                                                   
		    -- sSqlB /* 출력 B 조건이 들어가야함 */
							         AND (( B.BOOK_CODE1 =  'A4'
					           
					           
					              	)                                                                                   
				           		OR (B.BOOK_CODE2 = 'A4'    
					           
					           
					              ))
					           					
			        				 AND     B.DIV_CODE IN
										 (  
										 '01'
										 ) 									
					 		                                                                                         
					    	   
					    	   
					          		 AND B.AC_DATE >= '20160801'                                  
					          		 AND B.AC_DATE <= '20160816'                                      
					          		 AND B.INPUT_PATH != 'A1' 
         				 --- 출력 B 조건 끝 ----
		                          ), 0) AS CR_FOR_AMT_I                                                     
		                 , ISNULL(( SELECT SUM(ISNULL(B.CR_AMT_I, 0))                                       
		                              FROM ABA400T                                                          
		                             WHERE COMP_CODE = 'MASTER'
		                               AND ACCNT     = B.ACCNT                                                  
		    -- sSqlB /* 출력 B 조건이 들어가야함 */
							         AND (( B.BOOK_CODE1 =  'A4'
					           
					           
					              	)                                                                                   
				           		OR (B.BOOK_CODE2 = 'A4'   
					           
					           
					              ))
					           					
			        				 AND     B.DIV_CODE IN
										 (  
										 '01'
										 ) 									
					 		                                                                                         
					    	   
					    	   
					          		 AND B.AC_DATE >= '20160801'                                  
					          		 AND B.AC_DATE <= '20160816'                                      
					          		 AND B.INPUT_PATH != 'A1' 
         				 --- 출력 B 조건 끝 ----
		                          ), 0) AS CR_AMT_I                                                         
		                 , ISNULL((CASE B.JAN_DIVI WHEN '1' THEN SUM(ISNULL(B.DR_FOR_AMT_I, 0)) - SUM(ISNULL(B.CR_FOR_AMT_I, 0)) 
		                                           WHEN '2' THEN SUM(ISNULL(B.CR_FOR_AMT_I, 0)) - SUM(ISNULL(B.DR_FOR_AMT_I, 0)) 
		                           END                                                                      
		                          ), 0) AS JAN_FOR_AMT_I                                                    
		                 , ISNULL((CASE B.JAN_DIVI WHEN '1' THEN SUM(ISNULL(B.DR_AMT_I, 0)) - SUM(ISNULL(B.CR_AMT_I, 0)) 
		                                           WHEN '2' THEN SUM(ISNULL(B.CR_AMT_I, 0)) - SUM(ISNULL(B.DR_AMT_I, 0)) 
		                           END                                                                      
		                          ), 0) AS JAN_AMT_I                                                        
		              FROM ABA400T A       																	-- 20041108 agb200t 에서 agb200tv 으로 변경 khy
		                           LEFT OUTER JOIN AGB200TV B                                               
		                                        ON B.COMP_CODE = 'MASTER'
		                                       AND B.ACCNT     = A.ACCNT                                        
		                           LEFT OUTER JOIN BOR120T C                                                
		                                        ON C.COMP_CODE = 'MASTER'
		                                       AND B.DIV_CODE  = C.DIV_CODE                                  
		             WHERE A.COMP_CODE = 'MASTER'
		               AND B.ACCNT IS NOT NULL                                                              
		   
		               AND ((B.BOOK_CODE1 = 'A4'                               
		    
		     
		     
		                   )                                                                                
		    
		                OR (B.BOOK_CODE2 = 'A4'                                
		     
		     
		                   ))                                                                               
		    
		     					
	        				  AND     B.DIV_CODE IN
					 (  
					 '01'
					 ) 									
 		    
 		    
	  	 	 
	  	 			   AND B.AC_DATE >= '201504'                                
		               AND B.AC_DATE <= '20160816'                       
		             GROUP BY A.ACCNT, A.ACCNT_NAME      
		             
		                                                                               
	                 , A.ACCNT_NAME   
	                  
	                  
	                  
		             
		                   
		                    , B.DIV_CODE, C.DIV_NAME, B.ACCNT, B.JAN_DIVI, B.AC_DATE                        
		                    , B.BOOK_DATA1, B.BOOK_DATA2, B.BOOK_CODE1, B.BOOK_CODE2, B.INPUT_PATH          
		           ) A                                                                                      
		     WHERE ISNULL(A.PEND_CD,'') != ''                                                               
		     GROUP BY A.PEND_CD,A.PEND_NAME,A.ACCNT, A.ACCNT_NAME 	 
									     			                                          
		    HAVING SUM(WAL_AMT_I) != 0                                                            
		        OR SUM(DR_AMT_I)  != 0                                                             
		        OR SUM(CR_AMT_I)  != 0                                                             
		        OR SUM(JAN_AMT_I) != 0                                                            
		     ORDER BY A.PEND_CD, A.PEND_NAME, A.ACCNT]]>
	</queryString>
