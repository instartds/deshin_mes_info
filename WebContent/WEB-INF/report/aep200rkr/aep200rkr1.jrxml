	<queryString language="SQL">
		<![CDATA[--UArpt04Krv.Cagb100SKR [fnagb100P] Query3
SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
SELECT '1' GUBUN
     , '000000' GUBUN1
     , A.ACCNT
     , (SELECT ACCNT_NAME  
          FROM ABA400T 
         WHERE COMP_CODE = N'MASTER'
           AND ACCNT     = A.ACCNT) ACCNT_NAME
     , CONVERT(NVARCHAR(20), N'이월금액') AS AC_DATE
     , ISNULL(SUM(A.DR_AMT_I),0) DR_AMT_TOT_I
     , ISNULL(SUM(A.CR_AMT_I),0) CR_AMT_TOT_I
     , ISNULL(SUM(CASE A.JAN_DIVI WHEN '1' 
                                  THEN (A.DR_AMT_I) - (A.CR_AMT_I)
                                  WHEN '2' 
                                  THEN (A.CR_AMT_I) - (A.DR_AMT_I)
                   END),0) AS B_AMT_I
     , A.JAN_DIVI
  FROM AGB100TV A
     , BOR120T  B
 WHERE A.COMP_CODE = N'MASTER'
   AND B.COMP_CODE = N'MASTER'
   AND A.AC_DATE  >= N'201601'
   AND A.DIV_CODE  = B.DIV_CODE
   AND A.AC_DATE   < N'20161001'
     AND A.DIV_CODE IN ('01')
 GROUP  BY A.ACCNT,A.JAN_DIVI
 HAVING SUM(A.DR_AMT_I) <> 0 OR SUM(A.CR_AMT_I)<>0 

 UNION

SELECT '2' GUBUN
     , SUBSTRING(A.AC_DATE,1,6) GUBUN1
     , A.ACCNT
     , (SELECT ACCNT_NAME  
          FROM ABA400T 
         WHERE COMP_CODE = N'MASTER'
           AND ACCNT     = A.ACCNT) ACCNT_NAME
     , N'거래합계' AC_DATE
     , ISNULL(SUM(A.DR_AMT_I),0) DR_AMT_TOT_I
     , ISNULL(SUM(A.CR_AMT_I),0) CR_AMT_TOT_I
     , ISNULL(SUM(CASE A.JAN_DIVI WHEN '1' THEN (A.DR_AMT_I) - (A.CR_AMT_I)
                  WHEN '2' THEN (A.CR_AMT_I) - (A.DR_AMT_I)
                   END),0) AS B_AMT_I
     , A.JAN_DIVI
  FROM AGB100TV A
     , BOR120T  B
 WHERE A.COMP_CODE    = N'MASTER'
   AND B.COMP_CODE    = N'MASTER'
   AND A.DIV_CODE     = B.DIV_CODE
   AND A.INPUT_PATH   = 'A1' 
   AND LEN(A.AC_DATE) = 6
   AND A.AC_DATE between N'20161001' AND N'20161022'
   AND A.DIV_CODE IN ('01')
 GROUP  BY A.ACCNT,A.JAN_DIVI, A.AC_DATE
 HAVING SUM(A.DR_AMT_I) <> 0 OR SUM(A.CR_AMT_I)<>0 

 UNION

SELECT '3' GUBUN
     , SUBSTRING(A.AC_DATE,1,6) GUBUN1
     , A.ACCNT
     , B.ACCNT_NAME 
     , unilite.fnGetUserDateComp(N'MASTER', A.AC_DATE) AC_DATE
     , ISNULL(SUM(A.DR_AMT_I),0) DR_AMT_TOT_I
     , ISNULL(SUM(A.CR_AMT_I),0) CR_AMT_TOT_I
     , ISNULL(SUM(CASE A.JAN_DIVI WHEN '1' THEN (A.DR_AMT_I) - (A.CR_AMT_I)
                                  WHEN '2' THEN (A.CR_AMT_I) - (A.DR_AMT_I)
                   END),0) AS B_AMT_I
     , A.JAN_DIVI
  FROM AGB100TV A
     , ABA400T  B
     , BOR120T  E
 WHERE A.COMP_CODE = N'MASTER'
   AND B.COMP_CODE = N'MASTER'
   AND E.COMP_CODE = N'MASTER'
   AND A.ACCNT     = B.ACCNT
   AND A.DIV_CODE  = E.DIV_CODE
   AND ( LEN(A.AC_DATE) <> 6 OR A.AC_DATE = N'201601' )
         AND A.AC_DATE >= N'20161001'
         AND A.AC_DATE <= N'20161022'
             AND A.DIV_CODE IN ('01')
 GROUP  BY A.ACCNT,B.ACCNT_NAME  ,A.JAN_DIVI,A.AC_DATE
 HAVING SUM(A.DR_AMT_I) <> 0 OR SUM(A.CR_AMT_I)<>0                      

 UNION

SELECT '4' GUBUN
     , SUBSTRING(A.AC_DATE,1,6) GUBUN1
     , A.ACCNT
     , B.ACCNT_NAME 
     , CONVERT(NVARCHAR(20), N'소계(월)') AS AC_DATE
     , ISNULL(SUM(A.DR_AMT_I),0) DR_AMT_TOT_I
     , ISNULL(SUM(A.CR_AMT_I),0) CR_AMT_TOT_I
     , ISNULL(SUM(CASE A.JAN_DIVI WHEN '1' THEN (A.DR_AMT_I) - (A.CR_AMT_I)
                                  WHEN '2' THEN (A.CR_AMT_I) - (A.DR_AMT_I)
                   END),0) AS B_AMT_I 
     , A.JAN_DIVI
  FROM AGB100T A
     , ABA400T B
     , BOR120T E
 WHERE A.COMP_CODE = N'MASTER'
   AND B.COMP_CODE = N'MASTER'
   AND E.COMP_CODE = N'MASTER'
   AND A.ACCNT     = B.ACCNT
   AND A.DIV_CODE  = E.DIV_CODE
   AND ( LEN(A.AC_DATE) <> 6 OR A.AC_DATE = N'201601' )
   AND A.AC_DATE  >= N'20161001'
   AND A.AC_DATE  <= N'20161022'
   AND A.DIV_CODE IN ('01')
 GROUP BY A.ACCNT,B.ACCNT_NAME ,A.JAN_DIVI ,SUBSTRING(A.AC_DATE,1,6)
 ORDER BY 3,2,1,4]]>
	</queryString>
