	<queryString language="SQL">
		<![CDATA[SELECT 
    A.COMP_CODE
   , a.order_num
   , A.PROJECT_NO 
   , B.ORDER_SEQ
   , B.ITEM_CODE
   , F.ITEM_NAME
   , F.SPEC
   , B.ORDER_UNIT_Q
   , B.ORDER_UNIT
   , B.ORDER_UNIT_P
   , B.ORDER_O
   ,(
    SELECT TREE_NAME
    FROM BSA220T
    WHERE COMP_CODE = A.COMP_CODE
     AND B.WH_CODE = TREE_CODE
    ) AS WH_CODE
   ,uniLITE.fnGetUserDateComp(A.COMP_CODE, B.DVRY_DATE) AS DVRY_DATE
   , A.REMARK
   , G.COMP_NAME
   , A.ORDER_NUM
   , A.PROJECT_NO
   ,uniLITE.fnGetUserDateComp(A.COMP_CODE, A.ORDER_DATE) AS ORDER_DATE
   ,(
   SELECT CODE_NAME
   FROM BSA100T
   WHERE COMP_CODE = A.COMP_CODE
    AND A.ORDER_PRSN = SUB_CODE
    AND MAIN_CODE = 'M201'
   )       AS ORDER_PRSN
   , C.CUSTOM_NAME
   ,(
   SELECT CODE_NAME
   FROM BSA100T
   WHERE COMP_CODE = A.COMP_CODE
    AND C.SET_METH = SUB_CODE
    AND MAIN_CODE = 'B038'
   )       AS SET_METH
   
   , C.TELEPHON          AS C_TELEPHON
   , C.FAX_NUM     AS C_FAX_NUM
   ,(
   SELECT DIV_NAME
   FROM BOR120T
   WHERE COMP_CODE = A.COMP_CODE
    AND A.DIV_CODE = DIV_CODE
   ) AS DIV_CODE
   , D.ADDR
   , D.REPRE_NAME
   , D.TELEPHON        AS D_TELEPHON
   , D.FAX_NUM     AS D_FAX_NUM
   , D.COMP_NAME
   , A.REMARK
   
   FROM MPO100T A 
   
   INNER JOIN MPO200T B ON A.COMP_CODE    = B.COMP_CODE
                        AND A.DIV_CODE    = B.DIV_CODE
                        AND A.CUSTOM_CODE = B.CUSTOM_CODE
                        AND A.ORDER_NUM   = B.ORDER_NUM
                        
   INNER JOIN BCM100T C ON A.COMP_CODE = C.COMP_CODE
                        AND A.CUSTOM_CODE = C.CUSTOM_CODE
                        
   INNER JOIN BOR100T D ON A.COMP_CODE = D.COMP_CODE
   
   INNER JOIN BPR100T F ON F.COMP_CODE = A.COMP_CODE
                       AND F.ITEM_CODE = B.ITEM_CODE
   INNER JOIN BOR100T G ON A.COMP_CODE = G.COMP_CODE

  
   
   
   WHERE A.COMP_CODE = 'MASTER'
   AND   A.ORDER_NUM = 'M20150206005']]>
	</queryString>
