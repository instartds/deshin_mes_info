<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="agc190skrService">
	<select id="agc190skrService.fnCheckExistABA131" parameterType="Map" resultType="rMap">
		SELECT TOP 1  DIVI
	      FROM ABA131T WITH (NOLOCK)
	     WHERE COMP_CODE = #{S_COMP_CODE}
	       AND GUBUN     = #{GUBUN}		-- 재무제표양식차수
	       AND DIVI      = #{DIVI} 		-- Tab number
	</select>
	
	
	<select id="agc190skrService.selectList1" parameterType="Map" resultType="rMap">
		
	</select>
	
	<select id="agc190skrService.selectList2" parameterType="Map" resultType="rMap">
		
	</select>
	
	<select id="agc190skrService.selectList3" parameterType="Map" resultType="rMap">
		
	</select>
</mapper>