<?xml version="1.0" encoding="UTF-8"  ?>
<!DOCTYPE excelUpload PUBLIC
         "-//TLab//DTD excep upload config XML V1.0//EN"
         "../excel-upload.dtd">
<workBook name="bcm103" desc="주소정보"  validateService="bcm103ukrvService.excelValidate">
    <sheet seq="0" name="주소정보" startRow="1" sqlId="bcm103ukrvService.insertExcelBcm320t">
        <field col="0" name="OLD_ADDR"  title="입력주소"   type="string" required="true" check="true">
        	<comments>구 주소를 입력해 주세요</comments>
            <samples>
                <sample>서울특별시 구로구 구로동 코오롱빌란트2차 913호</sample>
                <sample>위 2~3행 데이터는 예제데이타입니다. 작업시 2~3행 데이터는 삭제하시고 작업하세요</sample>
            </samples>
        </field>
        <field col="1" name="OLD_ZIP_CODE" 	title="우편번호" 		type="string"  required="true">
        	<comments>우편번호를 입력해 주세요</comments>
            <samples>
                <sample>123-456</sample>
                <sample>000-000</sample>
            </samples>
        </field>
        <field col="2" name="NEW_ZIP_CODE"  title="새우편번호" type="string"  required="true">
            <comments>새 우편번호를 입력해주세요</comments>
            <samples>
                <sample>00000</sample>
                <sample>12345</sample>
            </samples>
        </field>
        <field col="3" name="NEW_ADDR1" 	title="도로명" 	type="string"  required="true">
        	<comments>도로명 주소를 입력해 주세요</comments>
            <samples>
                <sample>서울특별시 구로구 디지털로 30길 31</sample>
            </samples>
        </field>
        <field col="4" name="NEW_ADDR2" 	title="기타주소" 	type="string"  required="true">
        	<comments>상세주소를 입력해 주세요</comments>
            <samples>
                <sample>코오롱빌란트 2차 913호</sample>
            </samples>
        </field>
    </sheet>
</workBook>