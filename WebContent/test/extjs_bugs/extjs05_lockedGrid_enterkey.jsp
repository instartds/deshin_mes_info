Ext.define('Ext.override.panel.Table', {
    override: 'Ext.panel.Table',
    
    handleFocusEnter: function (e) {
        var me = this,
            view = me.getView(),
            targetView, navigationModel = view.getNavigationModel(),
            lastFocused, focusPosition, br = view.bufferedRenderer,
            firstRecord;
        
        if (!me.containsFocus && view.all.getCount()) {
            lastFocused = focusPosition = view.getLastFocused();
            
            if (!focusPosition) {
                targetView = view.isLockingView ? (view.lockedGrid.isVisible() ? view.lockedView : view.normalView) : view;
                firstRecord = view.dataSource.getAt(br ? br.getFirstVisibleRowIndex() : 0);
                if (!firstRecord.isNonData) {
                    focusPosition = new Ext.grid.CellContext(targetView).setPosition({
                        row: firstRecord,
                        column: 0
                    });
                }
            }
            if (!focusPosition) {
                e.stopEvent();
                e.getTarget().blur();
                return;
            }
            navigationModel.setPosition(focusPosition, null, e, null, !! lastFocused);
            me.containsFocus = !! navigationModel.getPosition();
        }
        if (me.containsFocus) {
            this.getView().el.dom.setAttribute('tabindex', '-1');
        }
    }
});

Ext.define('Ext.overide.grid.plugin.CellEditing', {
    override: 'Ext.grid.plugin.CellEditing',	
    
    onSpecialKey: function(ed, field, e) {
        var sm;
 		// Enter키를 Tab과 같이 사용
        if (e.getKey() === e.TAB || e.getKey() === e.ENTER) {
            e.stopEvent();

            if (ed) {
                // Allow the field to act on tabs before onEditorTab, which ends
                // up calling completeEdit. This is useful for picker type fields.
                ed.onEditorTab(e);
                var view = ed.editingPlugin.context.view,
            			grid = ed.editingPlugin.context.grid,
			            position = ed.editingPlugin.context,
			            direction = e.shiftKey ? 'left' : 'right';
                if(e.getKey() === e.ENTER){
            		if(ed && ed.editingPlugin && ed.editingPlugin.getActiveColumn()) {
            			ed.editingPlugin.getActiveColumn( ).field.focus(true);
				    }
            	
            	}
            	sm = ed.up('tablepanel').getSelectionModel();
            	var interValTime = 500;
            	// 팝업 필드의 경우 코드/코드명 가져오는 시간 기다리기 위해 interval을 줌.
            	if(field.getXType() == 'uniPopupColumn')	{
            	var chkInterval = 0;
            	chkInterval = setInterval(function(){
            											console.log("chkInterval:",chkInterval);
            											if(!Ext.getBody().isMasked()) {
            												moveCell(ed, e, sm, position, grid, view)
            												window.clearInterval(chkInterval)
            												console.log("clear chkInterval:",chkInterval);
										            	}
            										}
            										, interValTime);
            		
            	} else {
            		moveCell(ed, e, sm, position, grid, view)
            	}
        	}else {
        	 	console.log("ed false");
        	}
            
        }
    }
});

function moveCell(ed, e, sm, position, grid, view)	{
	if (sm.onEditorTab) {
            
            	if(!ed.editingPlugin.context.grid.isLocked && !Ext.isDefined(grid.ownerGrid)  && position.colIdx < grid.headerCt.getVisibleGridColumns().length-1 /*|| (ed.editingPlugin.context.grid.isLocked && position.colIdx < grid.headerCt.getVisibleGridColumns().length-1 )*/ ){
	            	var r = sm.onEditorTab(ed.editingPlugin, e);
	            	
	            	
	            	if(e.getKey() === e.ENTER){
	            		if(ed && ed.editingPlugin && ed.editingPlugin.getActiveColumn()) {
	            			ed.editingPlugin.getActiveColumn().field.focus(true);
	            		}
						if(grid.getItemId().indexOf("-normal") > -1)	{
							grid = grid.up('#'+grid.getItemId().replace('-normal',''));
						}
	            		if(position.isEqual(ed.editingPlugin.context))	{
	            			if('tree-grid' != ed.editingPlugin.context.grid.getXType() 
	            			   && !Ext.isDefined(grid.up('uniDetailWindow'))
	            			   && (grid.uniOpt && grid.uniOpt.enterKeyCreateRow)
		            		)	{
	            		 		UniAppManager.app.onNewDataButtonDown();  
	            			}
	            		}
	            	}
	                return r;
            	}else if(  position.colIdx < grid.headerCt.getVisibleGridColumns().length-1)	{
            		var r = sm.onEditorTab(ed.editingPlugin, e);
	            	
	            	
	            	if(e.getKey() === e.ENTER){
	            		if(ed && ed.editingPlugin && ed.editingPlugin.getActiveColumn()) {
	            			ed.editingPlugin.getActiveColumn().field.focus(true);
	            		}
						if(grid.getItemId().indexOf("-normal") > -1)	{
							grid = grid.up('#'+grid.getItemId().replace('-normal',''));
						}
	            		if(position.isEqual(ed.editingPlugin.context))	{
	            			if('tree-grid' != ed.editingPlugin.context.grid.getXType() 
	            			   && !Ext.isDefined(grid.up('uniDetailWindow'))
	            			   && (grid.uniOpt && grid.uniOpt.enterKeyCreateRow)
		            		)	{
	            		 		UniAppManager.app.onNewDataButtonDown();  
	            			}
	            		}
	            	}
            	
            		
            	}else {
            		var eView, eRows, eGrid ;
            		if(view.isLockedView)	{
            			eView = grid.ownerGrid.getView().normalView;
            			eGrid = grid.ownerGrid.normalGrid;
            			eRows = grid.getSelectionModel().getSelection()[0]
            		}else {
            			eView = grid.ownerGrid.getView().lockedView ? grid.ownerGrid.getView().lockedView : grid.ownerGrid.getView();
            			eGrid = grid.ownerGrid.getView().lockedView ? grid.ownerGrid.lockedGrid : grid.ownerGrid  ;
            			eRows = view.dataSource.getAt(position.rowIdx+1);
            		}
            		
            		var colIdx = -1;
            		Ext.each(eGrid.columns, function(column, idx){
            			if(colIdx == -1 && Ext.isDefined(column.getEditor()))	{
            				colIdx = idx;
            			}
            		});
            		if(colIdx == -1 ) colIdx=0;
            		ed.completeEdit();
            		var focusPosition = new Ext.grid.CellContext(eView).setPosition({
                        row: eRows,
                        column: colIdx
                    });
                    
            		grid.ownerGrid.getView().getNavigationModel().setPosition(focusPosition);
            		
					
            		/*	            		
            		var normalGrid = grid.up().items.items[1];
        			var editable = true;
					var selModel = normalGrid.selModel;
					var selection = selModel.selection;
					if( selection &&  selection.column == position.colIdx &&  selection.row == position.rowIdx) {
						editable = true;
					} else {
						editable = false;
					}
					var expanderSelector = view.expanderSelector;
			        if ( editable && (!expanderSelector || !e.getTarget(expanderSelector)) ) {
			        	var	plugin = normalGrid.editingPlugin;
			        	if(plugin)	{
							if(e.getKey() == e.ENTER ){
								
				            	plugin.startEdit(normalGrid.getSelection(), normalGrid.ownerCt.getColumnManager().getHeaderIndex(0));
				            	
				            }
			        	}   
			        } 
            	*/}
            }
}
